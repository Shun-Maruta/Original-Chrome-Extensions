{
	"manifest_version": 3,
	"name": "2nd Table Reader",
	"version": "1.0",
	"description": "Simple Table Reader",
	"permissions": [
		"activeTab",
		"scripting",
		"clipboardWrite"
	],
	"host_permissions": [
		"<all_urls>"
	],
	"action": {
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"content.js"
			],
			"run_at": "document_idle"
		}
	]
}